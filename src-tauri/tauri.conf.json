{
  "package": {
    "productName": "tauri",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "app-src",
    "devPath": "app-src",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": ["binaries/app"],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "Generator for various JSON files, for use with Minecraft",
      "longDescription": "Generator for various JSON files, for use with Minecraft",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
        "fs": {
          "all": true,
          "writeFile": true,
          "readDir": true,
          "createDir": true,
          "renameFile": true
      }
    },
    "windows": [
      {
        "title": "jsongenerator",
        "width": 830,
        "height": 650,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    }
  }
}
